---
title: 'Kelengkapan Profil Member Komunitas'
currentUrl: 'kelengkapan-hobby'
group: 'komunitas'
---
extends layouts/layout
block header
  include partials/header
block footer 
  include partials/footer
block content
  main.content.container-fluid
    .mx-lg-7.py-5
      .row.g-4
        .col-3
          include partials/side-profile
        .col-9 
          .d-flex.flex-column.gap-4 
            .card.rounded-3.border-0
              .card-body.px-5.py-4
                +btnback(title)
            .card.rounded-3.border-0
              .card-body.px-5.py-4
                .text-center
                  h6.small.mb-0 KATEGORI SEKOLAH/KAMPUS ALUMNI 

            .card.rounded-3.border-0
              .card-body.p-5 
                .d-flex.flex-column.gap-5 
                  .text-end 
                    button.btn.btn-secondary(type="button") Cancel 
                    button.btn.btn-primary.ms-3(type="button") Simpan
                  hr.my-0
                  div 
                    label.form-label.small(for="fakultas") Fakultas
                    select#fakultas.select.wide(name="fakultas")
                      option(value="") Pilih Fakultas
                      option(value="1") Fakultas Teknik
                      option(value="2") Fakultas KEdokteran
                      option(value="3") FISIP
                      option(value="4") STKIP
                  div 
                    label.form-label.small(for="jurusan") Jurusan
                    select#jurusan.select.wide(name="jurusan")
                      option(value="") Pilih Jurusan
                      option(value="1") Jurusan Teknik
                      option(value="2") Jurusan Kedokteran
                      option(value="3") FISIP
                      option(value="4") STKIP
                  div 
                    label.form-label.small(for="angkatan") Angkatan
                    select#angkatan.select.wide(name="angkatan")
                      option(value="") Pilih Angkatan
                      option(value="1") Angkatan Teknik
                      option(value="2") Angkatan Kedokteran
                      option(value="3") FISIP
                      option(value="4") STKIP