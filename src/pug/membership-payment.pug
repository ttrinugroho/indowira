                                  span.flex-grow-1.flex-shrink-0="Bank Blu"
            .card.rounded-3.border-0
              .card-body.p-5
                div 
                  label.form-label(for="judul") Voucher
                  .form-control.d-flex.gap-4
                    input.form-control.border-0#judul(type="text" placeholder="Kode Voucher")
                    .flex-grow-1.flex-shrink-0
                      button.btn.btn-info.text-uppercase(type="button") apply voucher
            .card.rounded-3.border-0
              .card-body.p-5
                .mb-5 
                  label.form-label(for="judul") Payment Detail
                  .card.rounded-3
                    .card-body
                      div 
                        h6.small Paket Berkarya 
                        p.d-flex.mb-2
                          small.me-auto 1 Bulan 
                          small.ms-auto Rp. 10.000
                        p.d-flex.mb-2
                          small.me-auto Voucher
                          small.ms-auto -
                        hr
                        h6.fw-semibold.d-flex
                          span Total Pembayaran 
                          span.ms-auto Rp. 10.000
                .text-end 
                  button.btn.btn-primary.text-uppercase(type="button") bayar sekarang
block footer 
  include partials/footer