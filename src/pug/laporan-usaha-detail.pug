---
title: 'Detail Usaha Anggota Komunitas'
currentUrl: 'laporan-usaha-detail'
group: 'komunitas'
---
extends layouts/layout
block header
  include partials/header
block footer 
  include partials/footer
block content
  main.content.container-fluid
    .mx-lg-7.py-5
      .row.g-4
        .col-9 
          .d-flex.flex-column.gap-4 
            .card.rounded-3.border-0
              .card-body.px-5.py-4
                +btnback(title)
            .card.rounded-3.border-0
              .card-body.p-5
                .d-flex.flex-column.gap-5
                  .d-flex.align-items-center.justify-content-between 
                    .d-flex.gap-4.align-items-center
                      img.rounded-circle.object-fit-cover(src="/img/contents/bakso-aci.jpeg", alt="", height="40", width="40")
                      div 
                        h6.fw-normal Bakso Aci
                        div 
                          i.bi.bi-pin-fill.text-secondary(style="font-size: 10px;")
                          span.badge.fw-normal.text-secondary Jl. Darmajaya, Jakarta Timur
                    div 
                      button.btn.btn-success.text-uppercase(type="button")
                        i.bi.bi-whatsapp 
                        span.ms-2 Hubungi kami
                  hr.my-0
                  h5.fw-normal.mb-0 Produk 
                  .row.row-cols-3.g-4
                    each produk in [1,2,3]
                      .col 
                        .card
                          .card-body
                            .mb-2 
                              img.rounded-2.object-fit-cover.w-100(src="/img/contents/bakso-aci.jpeg", alt="bakso aci", width="259" height="197")
